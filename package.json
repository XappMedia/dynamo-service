{
  "name": "@xapp/dynamo-service",
  "author": "XAPPmedia",
  "license": "Apache-2.0",
  "version": "1.4.2",
  "description": "A dynamo help class which will help maintain data integrity.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/XappMedia/dynamo-service.git",
  "scripts": {
    "clean": "rm -rf ./node_modules && rm -rf ./outputs",
    "cleanInstall": "npm run clean && npm install",
    "cleanBuild": "npm run cleanInstall && npm run build",
    "build": "tsc",
    "pretest": "npm run build",
    "test": "nyc mocha './src/**/*.test.ts'",
    "circleci": "nyc mocha './src/**/*.ts'",
    "deploy": "standard-version --message 'v%s [skip ci]' && git push --follow-tags origin master && npm publish",
    "postversion": "git push && git push --tags",
    "prepublishOnly": "npm run build -- -p ./tsconfig.production.json"
  },
  "keywords": [
    "dynamo",
    "stentor"
  ],
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.6.8",
    "@types/runes": "^0.4.1",
    "@types/sinon": "^7.0.13",
    "@types/sinon-chai": "^3.2.2",
    "@xapp/config": "^0.2.3",
    "aws-sdk": "^2.495.0",
    "chai": "^4.2.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "standard-version": "^6.0.1",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3",
    "uuid4": "^1.1.4"
  },
  "dependencies": {
    "runes": "0.4.3",
    "slugify": "1.3.4"
  }
}
