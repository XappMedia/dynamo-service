{
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/*.js.map": true
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/outputs": true,
        "**/.nyc_output": true,
        "**/api.yaml": true
    },
    "files.trimTrailingWhitespace": true,
    "typescript.tsdk": "node_modules/typescript/lib",
    "tslint.enable": true,
    "tslint.autoFixOnSave": true,
    "cSpell.words": [
        "dynoservice"
    ],
    "license-header-manager.additionalCommentStyles": [{
        "extension": ".ts",
        "commentStart": "/**",
        "commentMiddle": " *",
        "commentEnd": " */"
    }]
}