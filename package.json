{
  "name": "@xapp/dynamo-service",
  "version": "0.1.6",
  "description": "A dynamo help class which will help maintain data integrity.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/XappMedia/dynamo-service.git",
  "scripts": {
    "clean": "rm -rf ./node_modules && rm -rf ./outputs",
    "cleanInstall": "npm run clean && npm install",
    "cleanBuild": "npm run cleanInstall && npm run build",
    "build": "tsc",
    "pretest": "npm run build",
    "test": "nyc mocha './src/test/**/*.ts'",
    "circleci": "nyc mocha './src/test/**/*.ts'",
    "deploy:minor": "npm version patch -m 'Minor update [skip ci]' && npm publish",
    "postversion": "git push && git push --tags",
    "prepublishOnly": "npm run build -- -p ./tsconfig.production.json"
  },
  "keywords": [
    "dynamo",
    "stentor"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.0.10",
    "@types/mocha": "^2.2.45",
    "@types/node": "^9.4.0",
    "@types/runes": "^0.4.1",
    "@types/sinon": "^4.1.2",
    "@types/sinon-chai": "^2.7.29",
    "@xapp/config": "^0.2.1",
    "aws-sdk": "^2.175.0",
    "chai": "^4.1.2",
    "mocha": "^4.1.0",
    "nyc": "^11.4.1",
    "prettier": "^1.12.1",
    "sinon": "^4.1.3",
    "sinon-chai": "^2.14.0",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "typescript": "3.4.5",
    "uuid4": "^1.0.0"
  },
  "dependencies": {
    "runes": "^0.4.3",
    "slugify": "1.3.4"
  }
